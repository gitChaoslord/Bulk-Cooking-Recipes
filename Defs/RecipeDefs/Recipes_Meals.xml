<?xml version="1.0" encoding="utf-8"?>
<Defs>
<!-- simple meal x5 -->
    <RecipeDef ParentName="CookMealBase">
        <defName>CookMealSimpleBulk</defName>
        <label>cook simple meal x5</label>
        <description>Cook 5 straightforward meals from one ingredient.</description>
        <jobString>Cooking 5 simple meals.</jobString>
        <workAmount>1350</workAmount>
        <workSpeedStat>CookSpeed</workSpeedStat>
        <requiredGiverWorkType>Cooking</requiredGiverWorkType>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_CookMeal</soundWorking>
        <allowMixingIngredients>true</allowMixingIngredients>
        <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
        <ingredients>
            <li>
            <filter>
                <categories>
                <li>FoodRaw</li>
                </categories>
            </filter>
            <count>2.5</count>
            </li>
        </ingredients>
        <products>
            <MealSimple>5</MealSimple>
        </products>
        <skillRequirements>
            <Cooking>6</Cooking>
        </skillRequirements>
    </RecipeDef>

